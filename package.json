{
  "name": "shinlog",
  "version": "1.0.0",
  "description": "",
  "scripts": {
    "font": "node src/utils/font.js",
    "eslint": "npx eslint . --fix",
    "build": "ELEVENTY_ENV=true npx @11ty/eleventy",
    "start": "npx @11ty/eleventy --serve --quiet",
    "clean": "rm -rf _site",
    "brenchmark": "DEBUG=Eleventy:Benchmark* ELEVENTY_ENV=true npx @11ty/eleventy"
  },
  "dependencies": {
    "@11ty/eleventy": "^0.12.1",
    "@11ty/eleventy-img": "^0.8.3",
    "async-mutex": "^0.3.1",
    "autoprefixer": "^10.2.5",
    "csso": "^4.2.0",
    "fast-glob": "^3.2.5",
    "fs-extra": "^9.1.0",
    "html-minifier": "^4.0.0",
    "linkedom": "^0.7.3",
    "luxon": "^1.26.0",
    "markdown-it": "^12.0.6",
    "markdown-it-anchor": "^7.1.0",
    "markdown-it-attrs": "^4.0.0",
    "markdown-it-wrap-alphabet": "^1.3.0",
    "md5": "^2.3.0",
    "postcss": "^8.2.10",
    "remove-markdown": "^0.3.0",
    "rollup": "^2.45.2",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-url-resolve": "^0.1.1",
    "sass": "^1.32.11",
    "uslug": "^1.0.4"
  },
  "devDependencies": {
    "eslint": "^7.24.0",
    "gray-matter": "^4.0.2"
  },
  "eslintConfig": {
    "env": {
      "browser": true,
      "commonjs": true,
      "es2021": true
    },
    "ignorePatterns": [
      "_site/**/*.js",
      "!.eleventy.js"
    ],
    "extends": "eslint:recommended",
    "parserOptions": {
      "ecmaVersion": 12,
      "sourceType": "module"
    },
    "rules": {
      "indent": [
        "error",
        2
      ],
      "linebreak-style": [
        "error",
        "unix"
      ],
      "quotes": [
        "error",
        "single"
      ],
      "semi": [
        "error",
        "never"
      ],
      "arrow-parens": [
        "error",
        "as-needed"
      ],
      "arrow-spacing": "error",
      "curly": [
        "error",
        "multi"
      ]
    }
  },
  "author": "Alex Zhou",
  "license": "ISC"
}
